    "@typescript-eslint/parser": "^7.9.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lerna": "^8.1.3",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
