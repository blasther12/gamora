{
  "name": "gamora",
  "version": "1.0.0",
  "description": "finance control project",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "18 || 20"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "sb:run": "yarn workspace storybook storybook",
    "dev:frontend": "yarn workspace app dev",
    "dev:backend": "yarn workspace backend start:dev",
    "dev": "concurrently \"yarn workspace backend start:dev\" \"yarn workspace app dev\"",
    "build:frontend": "yarn workspace app build",
    "build:backend": "yarn workspace backend build",
    "build:all": "yarn workspaces foreach -A run build",
    "test:all": "yarn workspace backend test:cov",
    "prettier:fix": "prettier --write .",
    "lint:fix": "eslint --fix .",
    "schema:sync": "yarn workspace backend schema:sync",
    "typeorm:cache": "yarn workspace backend typeorm:cache",
    "schema:drop": "yarn workspace backend schema:drop",
    "migration:create": "yarn workspace backend migration:create",
    "migration:generate": "yarn workspace backend migration:generate",
    "migration:show": "yarn workspace backend migration:show",
    "migration:run": "yarn workspace backend migration:run",
    "migration:revert": "yarn workspace backend migration:revert",
    "prepare": "husky",
    "postinstall": "husky install || true"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.5",
    "@nrwl/workspace": "^19.2.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/supertest": "^6.0.2",
    "@types/webpack-env": "^1.18.5",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.11.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "lerna": "^8.1.3",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "packageManager": "yarn@4.2.2"
}
