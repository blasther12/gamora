{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "schema:sync": "yarn typeorm  schema:sync",
    "typeorm:cache": "yarn typeorm  cache:clear",
    "schema:drop": "yarn typeorm  -d ./src/infrastructure/config/typeorm.config.ts",
    "migration:create": "yarn typeorm  migration:create ./src/migrations/schema-update",
    "migration:generate": "yarn typeorm  migration:generate ./src/migrations/schema-update -d ./src/infrastructure/config/typeorm.config.ts",
    "migration:show": "yarn typeorm  migration:show -d ./src/infrastructure/config/typeorm.config.ts",
    "migration:run": "yarn typeorm  migration:run -d  ./src/infrastructure/config/typeorm.config.ts",
    "migration:revert": "yarn typeorm  migration:revert -d ./src/infrastructure/config/typeorm.config.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "@types/dotenv": "^8.2.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "objection": "^3.1.4",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5",
    "@types/express": "^4.17.17",
    "@types/jest": "^*",
    "@types/node": "^*",
    "@types/pg": "^8",
    "@types/supertest": "^*",
    "@typescript-eslint/eslint-plugin": "^*",
    "@typescript-eslint/parser": "^*",
    "eslint": "^*",
    "eslint-config-prettier": "^*",
    "eslint-plugin-prettier": "^*",
    "jest": "^*",
    "jest-mock-extended": "^*",
    "prettier": "^*",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.7",
    "supertest": "^*",
    "ts-jest": "^*",
    "ts-loader": "^*",
    "ts-node": "^*",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^*",
    "typescript": "^*"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
